{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "ignorePatterns": [
        "assets/chart.js"
    ],
    "rules": {
        "no-prototype-builtins": 0,
        "no-console": 1,
        "no-unused-vars": 1,
        "no-undef": 1
    },
    "globals": {
        "wpuerrorlogs_errors_by_hour": true,
        "wpuerrorlogs_options": true,
        "wp": true,
        "Chart": true,
        "jQuery": true
    }
}
